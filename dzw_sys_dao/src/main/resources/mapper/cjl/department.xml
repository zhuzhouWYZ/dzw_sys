<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.accp.dao.cjl.DepartmentDao">
  <select id="queryAll" resultType="com.accp.pojo.Department" >
	    SELECT 
	  `departmentid`,
	  `depnames` 
		FROM
	  `dzw`.`department` 
	    
  </select>
  <insert id="insert" parameterType="com.accp.pojo.Department">
  	INSERT INTO `dzw`.`department` (`depnames`) VALUES (#{depnames,jdbcType=VARCHAR}) ;
  </insert>
  <update id="update" parameterType="com.accp.pojo.Department">
  		UPDATE   `dzw`.`department` SET  `depnames` = #{depnames} WHERE `departmentid` = #{departmentid} ;
  </update>
  <delete id="delete" >
  		DELETE FROM  `dzw`.`department` WHERE `departmentid` = #{departmentid} ;
  </delete>
</mapper>
