<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.accp.dao.xn.SyslogDao">

	<select id="queryAllSyslog" resultType="com.accp.pojo.Syslog">
		SELECT * FROM `syslog` ORDER BY `create_time` DESC
	</select>
	
	<insert id="addSyslog">
		INSERT INTO `syslog` 
		(`user_id`,`user_action`,`create_time`) 
		VALUES
		(#{user_id}, #{user_action}, NOW())
	</insert>
	

	
</mapper>